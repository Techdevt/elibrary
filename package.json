{
  "name": "elibrary",
  "version": "0.0.1",
  "description": "An Isomorphic Elibrary Application",
  "main": "index.js",
  "engines": {
    "node": ">=5.4.1",
    "npm": ">=3.3.12"
  },
  "scripts": {
    "test": "NODE_PATH=$NODE_PATH:./shared npm run test:coverage",
    "wtest": "set NODE_PATH=./shared&& npm run test:coverage",
    "test:coverage": "./node_modules/.bin/babel-node ./node_modules/isparta/bin/isparta cover ./node_modules/mocha/bin/_mocha -- --require ./test/test_helper.js --bail --recursive test/**/**.spec.js",
    "test:watch": "npm run test -- --watch",
    "utest": "set NODE_PATH=./shared&&mocha --require . --recursive",
    "wstart": "set NODE_ENV=production&& set NODE_PATH=./shared&& pm2 start index.js --node-args='--harmony'",
    "wdev": "set NODE_PATH=./shared&& node --harmony .",
    "wbuild": "set NODE_ENV=production&& webpack --progress --color -p --config webpack.prod.config.js",
    "lstart": "NODE_ENV=production NODE_PATH=$NODE_PATH:./shared pm2 start index.js --node-args='--harmony'",
    "ldev": "NODE_PATH=$NODE_PATH:./shared node --harmony .",
    "lbuild": "NODE_ENV=production webpack --progress --color -p --config webpack.prod.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fanky5g/elibrary.git"
  },
